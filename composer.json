{
    "name": "mawdoo3/laravelTask",
    "description": "Mawdoo3 laravel Task",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ahmad athamneh",
            "email": "zaxx44a@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "mawdoo3\\laravelTask\\": "src/",
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --provider='mawdoo3\\laravelTask\\ContactServiceProvider'",
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "mawdoo3\\laravelTask\\ServiceProvider",
            ]
        }
    },
}
